# Same test as //test/builtin:execfd_test

(: executable_bytes_file Filename .self.opt.input_exe)

$(O exe) zec / / $(XOF executable_bytes_file)
$(O hello) zec / "hello" /
$(O world) zec / "world" /
$(O expect) zec / "hello there world" /

|< $(XF exe) / $(XA hello) " there " $(XA world) /
|- cmp $(XF expect)
|> stdout
